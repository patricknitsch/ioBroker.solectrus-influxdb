{
  "name": "iobroker.solectrus-influxdb",
  "version": "1.1.1",
  "description": "Configure Sensors for SOLECTRUS and push to InfluxDB",
  "author": {
    "name": "patricknitsch",
    "email": "patricknitsch@web.de"
  },
  "contributors": [
    {
      "name": "Felliglanz"
    }
  ],
  "homepage": "https://github.com/patricknitsch/ioBroker.solectrus-influxdb",
  "license": "MIT",
  "keywords": [
    "solectrus",
    "influxdb",
    "ioBroker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/patricknitsch/ioBroker.solectrus-influxdb"
  },
  "engines": {
    "node": ">= 20"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@iobroker/adapter-core": "^3.3.2",
    "jsep": "^1.4.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^5.0.0",
    "@alcalzone/release-script-plugin-iobroker": "^4.0.0",
    "@alcalzone/release-script-plugin-license": "^4.0.0",
    "@alcalzone/release-script-plugin-manual-review": "^4.0.0",
    "@iobroker/adapter-dev": "^1.5.0",
    "@iobroker/dev-server": "^0.8.0",
    "@iobroker/eslint-config": "^2.2.0",
    "@iobroker/testing": "^5.2.2",
    "@tsconfig/node20": "^20.1.8",
    "@types/iobroker": "npm:@iobroker/types@^7.1.0",
    "@types/node": "^20.19.28",
    "typescript": "^5.9.3"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).{json,json5}",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint -c eslint.config.mjs .",
    "translate": "translate-adapter",
    "release": "release-script --yes",
    "release-minor": "release-script minor --yes",
    "release-major": "release-script major --yes",
    "dev-server": "dev-server"
  },
  "bugs": {
    "url": "https://github.com/patricknitsch/ioBroker.solectrus-influxdb/issues"
  },
  "readmeFilename": "README.md"
}
